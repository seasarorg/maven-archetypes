<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="archetype" name="Archetype" schema="schema/archetype.exsd"/>
   <extension-point id="remoteRepository" name="remoteRepository" schema="schema/remoteRepository.exsd"/>
   <extension
         point="org.eclipse.ui.newWizards">
         <wizard
               category="Seasar Maven Archetypes"
               class="org.seasar.maven.archetypes.eclipse.plugin.NewProjectWizard"
               hasPages="true"
               id="org.seasar.maven.archetypes.eclipse.plugin.s2struts.mayaa.NewProjectWizard"
               name="Maven Archetype Project"
               project="true">
         </wizard>
   </extension>
   <extension
         point="seasar-maven-archetypes-eclipse-plugin.archetype">
         <archetype
               artifactId="maven-archetype-seasar-struts-mayaa"
               groupId="org.seasar.maven-archetypes"
               version="0.0.1"
               name="S2Struts+Mayaa">
         </archetype>
         <archetype
               artifactId="maven-archetype-seasar-struts2-mayaa"
               groupId="org.seasar.maven-archetypes"
               version="0.0.1"
               name="Struts2+Mayaa">
         </archetype>
   </extension>
   <extension
         point="seasar-maven-archetypes-eclipse-plugin.remoteRepository">
      <remoteRepository
            uri="http://maven.seasar.org/maven2/">
      </remoteRepository>
      <remoteRepository
            uri="https://www.seasar.org/maven/maven2-snapshot">
      </remoteRepository>
   </extension>
</plugin>
